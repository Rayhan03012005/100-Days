public class H63 {
    public static void main(String[] args) {
        cetakPola(5);
    }
    public static void cetakPola(int jumlahBaris) {
        for (int i = 1; i <= jumlahBaris; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}

public class H62{
    public static void main(String[] args) {
        int panjang = 5;

        for (int i = 1; i <= panjang; i++) {
            for (int j = 1; j <= panjang; j++) {
                if (i == panjang / 2 + 1 || j == panjang / 2 + 1) {
                    System.out.print(" +");
                } else {
                    System.out.print("  ");
                }
            }
            System.out.println();
        }
    }
}

import java.util.Scanner;
public class H81 {
    public static void main(String[] args) {
        Scanner input=new Scanner (System.in);
        
        System.out.print("masukan angka untuk di bagi 3 :");
        int jumlah=0;
        int angka = input.nextInt();
        for(int i =1;i<=angka;i++){
            if (i%3==0){
            jumlah++;
            }
        }
        System.out.println("jumlah angka yang bisa di bagi 3 dari 1 - "+angka+"  berjumlah : "+jumlah);
    }
}
